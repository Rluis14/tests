@model W04_Phase_01.Models.UserInfo

@{
    ViewBag.Title = "User Information Submitted";
}

<h2>Submitted Information</h2>

<dl class="dl-horizontal">
    <dt>First Name:</dt>
    <dd>@Model.FName</dd>

    <dt>Last Name:</dt>
    <dd>@Model.LName</dd>

    <dt>Email:</dt>
    <dd>@Model.Email</dd>

    <dt>Phone:</dt>
    <dd>@Model.Phone</dd>

    <dt>Address:</dt>
    <dd>@Model.Address</dd>

    <dt>Username:</dt>
    <dd>@Model.UserName</dd>

    <dt>Password:</dt>
    <dd>@Model.Password</dd>
</dl>